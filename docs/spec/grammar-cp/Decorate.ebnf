		Decorate(Type<?Variable>)
	);
Decorate(EntryType<+Named><+Optional><?Variable> ::= Word "?:" Type<?Variable>) -> SemanticPropertyType
	:= (SemanticPropertyType[optional=true]
		Decorate(Word)
		Decorate(Type<?Variable>)
	);



Decorate(ItemsType ::= EntryType<-Named><-Optional><?Variable># ","?) -> Sequence<SemanticItemType>
	:= ParseList(EntryType<-Named><-Optional><?Variable>, SemanticItemType);
Decorate(ItemsType ::= EntryType<-Named><+Optional><?Variable># ","?) -> Sequence<SemanticItemType>
	:= ParseList(EntryType<-Named><+Optional><?Variable>, SemanticItemType);
Decorate(ItemsType ::= EntryType<-Named><-Optional><?Variable># "," EntryType<-Named><+Optional><?Variable># ","?) -> Sequence<SemanticItemType>
	:= [
		...ParseList(EntryType<-Named><-Optional><?Variable>, SemanticItemType),
		...ParseList(EntryType<-Named><+Optional><?Variable>, SemanticItemType),
	];



Decorate(PropertiesType ::= EntryType<+Named><-Optional><?Variable># ","?) -> Sequence<SemanticPropertyType>
	:= ParseList(EntryType<+Named><-Optional><?Variable>, SemanticPropertyType);
Decorate(PropertiesType ::= EntryType<+Named><+Optional><?Variable># ","?) -> Sequence<SemanticPropertyType>
	:= ParseList(EntryType<+Named><+Optional><?Variable>, SemanticPropertyType);



Decorate(TypeGrouped<?Variable> ::= "(" Type<?Variable> ")") -> SemanticType
	:= Decorate(Type<?Variable>);

