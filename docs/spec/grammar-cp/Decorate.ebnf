Decorate(TypeUnarySymbol<Variable> ::= TypeCompound<?Variable>) -> SemanticType
	:= Decorate(TypeCompound<?Variable>);
Decorate(TypeUnarySymbol<Variable> ::= TypeUnarySymbol<?Variable> "?") -> SemanticTypeOperation
